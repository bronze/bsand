{
  "name": "11ty-sass-skeleton",
  "version": "3.0.1",
  "description": "Featuring absolutely nothing beyond a base HTML5 template and the essential setup to watch and compile your Sass alongside 11ty.",
  "main": "index.html",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "npm-run-all -p dev:*",
    "dev": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "dev:css": "tailwindcss -i src/css/tailwind.css -o dist/css/tailwind.css --watch --postcss",
    "build:11ty": "cross-env ELEVENTY_ENV=prod eleventy",
    "build:css": "tailwindcss -i src/css/tailwind.css -o dist/css/tailwind.css --postcss --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5t3ph/11ty-sass-skeleton.git"
  },
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "@11ty/is-land": "^4.0.0",
    "@11tyrocks/eleventy-plugin-sass-lightningcss": "^1.0.3",
    "clean-css": "^5.3.2",
    "eleventy-critical-css": "^1.1.0"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.3.5",
    "autoprefixer": "^10.4.14",
    "browserslist": "^4.21.5",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "lightningcss": "^1.20.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.2.4",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2"
  }
}
